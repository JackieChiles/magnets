<!doctype html>
<html ng-app="MagnetsApp">
  <head>
    <title>Mag, You're It!</title>
    <% include header.ejs %>
  </head>
  <body ng-controller="VisitsController" ng-init="magnetId = '<%= id %>'">
    <h1>Mag, You're It!</h1>

    <mag-visit ng-repeat="visit in magnet.visits" visit="visit"></mag-visit>

    <div>{{ magnetId }}</div>
  </body>
</html>
